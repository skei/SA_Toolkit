.PHONY: clean All

All:
	@echo "----------Building project:[ SA_Toolkit - linux_plugin_debug ]----------"
	@"$(MAKE)" -f  "SA_Toolkit.mk" && "$(MAKE)" -f  "SA_Toolkit.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ SA_Toolkit - linux_plugin_debug ]----------"
	@"$(MAKE)" -f  "SA_Toolkit.mk" clean
